{
    "name": "cakephp-extended/plugin-app-installer",
    "description": "Install CakePHP 3.0+ your plugins in plugins folder inside your App",
    "type": "composer-installer",
    "license": "MIT",
    "authors": [
        {
            "name": "Fernando Pita"
        }
    ],
    "autoload": {
        "psr-4": {
            "Custom\\Composer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Custom\\Test\\Composer\\": "tests"
        }
    },
    "extra": {
        "class": "Custom\\Composer\\Installer\\PluginAppInstaller"
    }
}
